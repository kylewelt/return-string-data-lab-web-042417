<%= form_for @product do |f| %>
  <%= f.label :name, "Product Name" %><br>
  <%= f.text_field :name %><br>
  <%= f.label :name, "Product Description" %><br>
  <%= f.text_area :description %><br>
  <%= f.label :name, "Product Inventory" %><br>
  <%= f.number_field :inventory %><br>
  <%= f.submit %>
<% end %>
